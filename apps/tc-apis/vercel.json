{
  "git": {
    "deploymentEnabled": {
      "experiment-*": false,
      "*-dev": true
    }
  },
  "redirects": [
    {
      "source": "/me",
      "destination": "/",
      "permanent": false
    }
  ],
  "functions": {
    "app/api/**/*": {
      "memory": 768
    },
    "app/index.*": {
      "memory": 512
    }
  }
}
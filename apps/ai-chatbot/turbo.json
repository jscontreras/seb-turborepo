{
  "extends": [
    "//"
  ],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "env": [
        "REDIS_URL",
        "BLOB_READ_WRITE_TOKEN",
        "AI_GATEWAY_API_KEY",
        "NANO_MODEL",
        "MINI_MODEL"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**",
        ".svelte-kit/**",
        "dist/**",
        ".vercel/**"
      ]
    },
    "dev": {
      "dependsOn": ["^build"],
      "env": [
        "REDIS_URL",
        "BLOB_READ_WRITE_TOKEN",
        "AI_GATEWAY_API_KEY",
        "NANO_MODEL",
        "MINI_MODEL"
      ]
    }
  }
}
{
  "extends": [
    "//"
  ],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "env": ["CUSTOM_API_KEY"],
      "outputs": [
        ".next/**",
        "!.next/cache/**",
        ".svelte-kit/**",
        "dist/**",
        ".vercel/**"
      ]
    },
    "dev": {
      "dependsOn": ["^build"],
      "env": ["CUSTOM_API_KEY"]
    }
  }
}
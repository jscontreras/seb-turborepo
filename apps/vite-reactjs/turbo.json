{
  "extends": [
    "//"
  ],
  "globalEnv": [
    "AUTH0_CLIENT_SECRET",
    "VITE_AUTH0_AUDIENCE",
    "VITE_AUTH0_CALLBACK_URL",
    "VITE_AUTH0_CLIENT_ID",
    "VITE_AUTH0_DOMAIN",
    "VITE_AUTH0_SCOPE"
  ],
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ]
    },
    "vercel": {
      "cache": false,
      "persistent": true,
      "dependsOn": [
        "^src"
      ],
      "outputs": [
        "src/**"
      ]
    }
  }
}